
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html lang="zxx">
<head>
    <!-- custom-theme -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Esteem Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //custom-theme -->
    <link href="/web3/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/web3/css/component.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/web3/css/export.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/web3/css/style_grid.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/web3/css/style.css" rel="stylesheet" type="text/css" media="all" />

    <!-- font-awesome-icons -->
    <link href="/web3/css/font-awesome.css" rel="stylesheet">
    <!-- //font-awesome-icons -->
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800" rel="stylesheet">
</head>
<body>
    <!-- banner -->
    <div class="clearfix"></div>

    <ul class="ca-menu">
        <li  style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-pie-chart" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main">@ViewBag.yemek </h4>
                    <h3 class="ca-sub">Yemek Sayımız</h3>
                </div>
            </a>
        </li>
        <li  style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-user" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main one">  @ViewBag.kat</h4>
                    <h3 class="ca-sub one">Kategori Sayımız</h3>
                </div>
            </a>
        </li>
        <li  style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-money" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main two">  @ViewBag.yorum</h4>
                    <h3 class="ca-sub two">Yorum Sayısı</h3>
                </div>
            </a>
        </li>
        <li  style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-apple" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main three"> @ViewBag.mesaj</h4>
                    <h3 class="ca-sub three">Mesaj Sayısı</h3>
                </div>
            </a>
        </li>

    </ul>

    <ul class="ca-menu">
        <li style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-list" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main">  @ViewBag.aktifyemek</h4>
                    <h3 class="ca-sub">Aktif Yemek Sayısı</h3>
                </div>
            </a>
        </li>
        <li style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-language" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main one"> @ViewBag.aktifkat</h4>
                    <h3 class="ca-sub one">Aktif Kategori Sayısı</h3>
                </div>
            </a>
        </li>
        <li style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-database" aria-hidden="true"></i>
                <div class="ca-content">
                    <h4 class="ca-main two">  @ViewBag.aktifyorum</h4>
                    <h3 class="ca-sub two">Aktif Yorum Sayısı </h3>
                </div>
            </a>
        </li>
        <li style="height:300px; width:250px;">
            <a href="#">
                <i class="fa fa-sticky-note" aria-hidden="true"></i>
                <div class="ca-content">

                    <h4 class="ca-main three">   @ViewBag.admin</h4>

                    <h3 class="ca-sub three">Aktif Admin Sayısı</h3>
                </div>
            </a>
        </li>
    </ul>
</body>
</html>
